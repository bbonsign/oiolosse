os_window_title qlair

# Set the working directory for windows in the current tab
cd ~/mh/qlair/backend.git/wt-main/
title code
launch --title code

new_tab ipy
cd ~/mh/qlair/backend.git/wt-main/
launch --title ipy

new_tab fe
cd ~/mh/qlair/webportal.git/main/
launch

new_tab fe-server
cd ~/mh/qlair/webportal.git/main/
launch --title "fe-server"

focus_matching_window title:code and session:.

