os_window_title conf

# Set the working directory for windows in the current tab
cd ~/oiolosse/
title oiolosse
launch --title "oiolosse"

new_tab telperion
cd ~/telperion/
launch --title "telperion"

new_tab conf 
cd ~/.config/
launch --title "conf"

focus_matching_window title:oiolosse and session:.

