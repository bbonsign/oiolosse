# Set the working directory for windows in the current tab
cd ~/mh/streametric/backend-v2.git/wts/dev/
title code
launch --title code

new_tab alt
cd ~/mh/streametric/backend-v2.git/wts/alt/
launch 

new_tab fe
cd ~/mh/streametric/backend-v2.git/wts/alt/
launch --title "fe-code"
launch --title "fe-server"

focus_matching_window title:code and session:.


